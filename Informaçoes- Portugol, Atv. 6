programa {
  funcao inicio() {
    cadeia nome, idade, altura
    escreva ("Qual e o seu nome?")
    leia (nome)
    escreva ("Qual e a sua idade?")
    leia (idade)
    escreva ("Qual e a sua altura?")
    leia (altura)
    escreva("\nSeu nome e: ", nome)
    escreva("\nSua idade e: " ,idade)
    escreva("\nSua altura e: " , altura)
    
    
  }
}
